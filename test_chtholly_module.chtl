                        script {
                            vir DataProcessor = iNeverAway({
                                "Transform<JSON>": function(data, options) {
                                    console.log('JSONè½¬æ¢å¤„ç†');
                                    const result = {
                                        type: 'JSON',
                                        timestamp: new Date().toISOString(),
                                        data: JSON.stringify(data, null, options?.indent || 2),
                                        size: JSON.stringify(data).length,
                                        formatted: true
                                    };
                                    
                                    {{.transform-output}}->textContent = `JSONè½¬æ¢ç»“æœ:\n${result.data}\n\nå…ƒæ•°æ®:\n- å¤§å°: ${result.size} å­—ç¬¦\n- æ—¶é—´: ${result.timestamp}`;
                                    return result;
                                },
                                
                                "Transform<XML>": function(data, options) {
                                    console.log('XMLè½¬æ¢å¤„ç†');
                                    const xmlData = `<?xml version="1.0" encoding="UTF-8"?>
<root>
    <metadata>
        <timestamp>${new Date().toISOString()}</timestamp>
        <type>XML</type>
    </metadata>
    <data>${JSON.stringify(data)}</data>
</root>`;
                                    
                                    {{.transform-output}}->textContent = `XMLè½¬æ¢ç»“æœ:\n${xmlData}`;
                                    return xmlData;
                                },
                                
                                "Transform<CSV>": function(data, options) {
                                    console.log('CSVè½¬æ¢å¤„ç†');
                                    let csvData = '';
                                    
                                    if (Array.isArray(data) && data.length > 0) {
                                        // è·å–è¡¨å¤´
                                        const headers = Object.keys(data[0]);
                                        csvData = headers.join(',') + '\n';
                                        
                                        // æ·»åŠ æ•°æ®è¡Œ
                                        data.forEach(row => {
                                            const values = headers.map(header => `"${row[header] || ''}"`);
                                            csvData += values.join(',') + '\n';
                                        });
                                    } else {
                                        csvData = 'Invalid data format for CSV conversion';
                                    }
                                    
                                    {{.transform-output}}->textContent = `CSVè½¬æ¢ç»“æœ:\n${csvData}`;
                                    return csvData;
                                },
                                
                                "Analyze<Structure>": function(data) {
                                    console.log('ç»“æ„åˆ†æå¤„ç†');
                                    const analysis = {
                                        type: typeof data,
                                        isArray: Array.isArray(data),
                                        keys: Object.keys(data || {}),
                                        depth: this.calculateDepth(data),
                                        size: JSON.stringify(data).length
                                    };
                                    
                                    const analysisText = `
ğŸ” æ•°æ®ç»“æ„åˆ†æ:
ğŸ“Š ç±»å‹: ${analysis.type}
ğŸ“‹ æ˜¯å¦æ•°ç»„: ${analysis.isArray}
ğŸ”‘ é”®æ•°é‡: ${analysis.keys.length}
ğŸ“ åµŒå¥—æ·±åº¦: ${analysis.depth}
ğŸ“¦ æ•°æ®å¤§å°: ${analysis.size} å­—ç¬¦
ğŸ—ï¸  é”®åˆ—è¡¨: ${analysis.keys.join(', ')}`;
                                    
                                    {{.transform-output}}->textContent = analysisText;
                                    return analysis;
                                },
                                
                                "Analyze<Performance>": function(data) {
                                    console.log('æ€§èƒ½åˆ†æå¤„ç†');
                                    const startTime = performance.now();
                                    
                                    // æ¨¡æ‹Ÿæ€§èƒ½åˆ†æ
                                    let operations = 0;
                                    for (let i = 0; i < 10000; i++) {
                                        JSON.stringify(data);
                                        operations++;
                                    }
                                    
                                    const endTime = performance.now();
                                    const duration = endTime - startTime;
                                    
                                    const perfText = `
âš¡ æ€§èƒ½åˆ†æç»“æœ:
â±ï¸  æ‰§è¡Œæ—¶é—´: ${duration.toFixed(2)}ms
ğŸ”„ æ“ä½œæ¬¡æ•°: ${operations}
ğŸ“ˆ æ“ä½œ/ç§’: ${(operations / (duration / 1000)).toFixed(0)}
ğŸ’¾ å†…å­˜ä½¿ç”¨: ~${(JSON.stringify(data).length * operations / 1024).toFixed(2)}KB
ğŸ¯ æ•ˆç‡è¯„çº§: ${duration < 100 ? 'ä¼˜ç§€' : duration < 500 ? 'è‰¯å¥½' : 'éœ€ä¼˜åŒ–'}`;
                                    
                                    {{.transform-output}}->textContent = perfText;
                                    return { duration, operations, efficiency: duration / operations };
                                },
                                
                                calculateDepth: function(obj, depth = 0) {
                                    if (obj === null || typeof obj !== 'object') {
                                        return depth;
                                    }
                                    
                                    let maxDepth = depth;
                                    for (const key in obj) {
                                        if (obj.hasOwnProperty(key)) {
                                            const currentDepth = this.calculateDepth(obj[key], depth + 1);
                                            maxDepth = Math.max(maxDepth, currentDepth);
                                        }
                                    }
                                    
                                    return maxDepth;
                                }
                            });
                        }
                        
