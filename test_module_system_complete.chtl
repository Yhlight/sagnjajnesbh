[Custom] @Element InteractiveModuleDemo {
    div {
        class: interactive-module;
        
        style {
            .interactive-module {
                background: "linear-gradient(135deg, #fdf2f8 0%, #fce7f3 100%)";
                border: "3px solid #ec4899";
                border-radius: "15px";
                padding: "30px";
                margin: "25px 0";
                position: "relative";
                overflow: "hidden";
            }
            
            &::before {
                content: "üíé";
                position: "absolute";
                top: "15px";
                right: "20px";
                font-size: "1.5rem";
                opacity: "0.6";
            }
        }
        
        script {
            vir ModuleInteractionManager = listen({
                state: {
                    interactions: 0,
                    lastAction: null,
                    startTime: Date.now()
                },
                
                recordInteraction: function(action) {
                    this.state.interactions++;
                    this.state.lastAction = action;
                    
                    const elapsed = Date.now() - this.state.startTime;
                    const minutes = Math.floor(elapsed / 60000);
                    const seconds = Math.floor((elapsed % 60000) / 1000);
                    
                    {{.interaction-counter}}->textContent = this.state.interactions;
                    {{.last-action}}->textContent = action;
                    {{.elapsed-time}}->textContent = `${minutes}:${seconds.toString().padStart(2, '0')}`;
                    
                    console.log(`Ê®°Âùó‰∫§‰∫íËÆ∞ÂΩï: ${action}, ÊÄªËÆ°: ${this.state.interactions}`);
                },
                
                animateInteraction: function() {
                    const target = {{.interactive-module}};
                    
                    animate({
                        target: target,
                        duration: 600,
                        easing: "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
                        
                        begin: {
                            transform: 'scale(1)',
                            borderColor: '#ec4899'
                        },
                        
                        when: [
                            {
                                at: 0.5,
                                transform: 'scale(1.02)',
                                borderColor: '#be185d'
                            }
                        ],
                        
                        end: {
                            transform: 'scale(1)',
                            borderColor: '#ec4899'
                        },
                        
                        callback: function() {
                            ModuleInteractionManager->recordInteraction('animation_complete');
                        }
                    });
                }
            });
            
            {{&}}->listen({
                click: function() {
                    ModuleInteractionManager->recordInteraction('container_click');
                    ModuleInteractionManager->animateInteraction();
                }
            });
        }
        
        h3 {
            text { ‰∫§‰∫íÂºèÊ®°ÂùóÊºîÁ§∫ }
            color: "#be185d";
            margin-bottom: "20px";
        }
        
        div {
            class: interaction-stats;
            
            style {
                .interaction-stats {
                    background: "rgba(255, 255, 255, 0.7)";
                    border: "1px solid rgba(236, 72, 153, 0.3)";
                    border-radius: "8px";
                    padding: "15px";
                    margin: "15px 0";
                    display: "grid";
                    grid-template-columns: "repeat(auto-fit, minmax(150px, 1fr))";
                    gap: "10px";
                }
                
                .stat-item {
                    text-align: "center";
                    padding: "10px";
                }
                
                .stat-value {
                    font-size: "1.5rem";
                    font-weight: "700";
                    color: "#be185d";
                    display: "block";
                }
                
                .stat-label {
                    font-size: "0.9rem";
                    color: "#6b7280";
                    margin-top: "5px";
                }
            }
            
            div {
                class: stat-item;
                
                span {
                    class: stat-value interaction-counter;
                    text { 0 }
                }
                
                span {
                    class: stat-label;
                    text { ‰∫§‰∫íÊ¨°Êï∞ }
                }
            }
            
            div {
                class: stat-item;
                
                span {
                    class: stat-value last-action;
                    text { Êó† }
                }
                
                span {
                    class: stat-label;
                    text { ÊúÄÂêéÊìç‰Ωú }
                }
            }
            
            div {
                class: stat-item;
                
                span {
                    class: stat-value elapsed-time;
                    text { 0:00 }
                }
                
                span {
                    class: stat-label;
                    text { ËøêË°åÊó∂Èó¥ }
                }
            }
        }
        
        p {
            text { ÁÇπÂáªÊ≠§Âå∫ÂüüÊµãËØïÊ®°Âùó‰∫§‰∫íÂäüËÉΩ }
